{
    "name": "morningtrain/wp-e-conomic",
    "description": "This is my package morning-train-wp-e-conomic",
    "keywords": [
        "Morningtrain",
        "morning-trian-wp-e-conomic"
    ],
    "homepage": "https://github.com/morningtrain/morningtrain/wp-e-conomic",
    "license": "MIT",
    "authors": [
        {
            "name": "Lars Rasmussen",
            "email": "laras@morningtrain.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "morning-train/e-conomic": "dev-main"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "MorningTrain\\WpEconomic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MorningTrain\\WpEconomic\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
